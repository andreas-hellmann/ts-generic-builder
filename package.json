{
  "name": "typescript-generic-builder",
  "version": "1.0.0",
  "description": "Type-safe generic builder pattern implementation in Typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanterlantant/ts-generic-builder"
  },
  "main": "dist/src/builder.js",
  "types": "dist/src/builder.d.ts",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine test/**/*.ts",
    "prettier": "prettier --config .prettierrc './**/*.ts' --write",
    "build": "rm -rf dist && tsc --build tsconfig.json",
    "lint": "eslint -c .eslintrc.js --ext .ts src"
  },
  "author": "Andreas Hellmann <hanterlantant@posteo.de>",
  "license": "MIT",
  "keywords": ["typescript", "type-safe", "generic", "builder pattern"],
  "devDependencies": {
    "@types/jasmine": "^3.5.9",
    "@types/node": "^13.9.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jasmine": "^3.5.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  }
}
